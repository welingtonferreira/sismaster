  object GraficoVendas: TFDQuery
    Connection = DMPrincipal.FireDacCon
    SQL.Strings = (
      'SELECT '
      '    YEAR(v.data_venda) AS Ano, '
      '    MONTH(v.data_venda) AS Mes, '
      
        '    COUNT(DISTINCT v.id_venda) AS Quantidade -- Contando as vend' +
        'as de forma distinta'
      'FROM '
      '    vendas v'
      '    LEFT JOIN itens_venda iv ON v.id_venda = iv.id_venda'
      '    LEFT JOIN clientes c ON v.id_cliente = c.id_cliente'
      '    LEFT JOIN produtos p ON iv.id_produto = p.id_produto'
      'WHERE '
      '    (v.data_venda BETWEEN :DataInicio AND :DataFim)'
      
        '    AND (v.id_cliente = :id_cliente OR :id_cliente = 9999)  -- C' +
        'aso 9999 seja um valor "todos"'
      
        '    AND (iv.id_produto = :id_produto OR :id_produto = 9999)  -- ' +
        'Caso 9999 seja um valor "todos"'
      'GROUP BY '
      '    YEAR(v.data_venda), MONTH(v.data_venda)'
      'ORDER BY '
      '    Ano, Mes;'
      
        '                                                                ' +
        '                           ')
    Left = 377
    Top = 33
    ParamData = <
      item
        Name = 'DATAINICIO'
        DataType = ftDate
        ParamType = ptInput
        Value = Null
      end
      item
        Name = 'DATAFIM'
        DataType = ftDate
        ParamType = ptInput
        Value = Null
      end
      item
        Name = 'ID_CLIENTE'
        DataType = ftInteger
        ParamType = ptInput
        Value = Null
      end
      item
        Name = 'ID_PRODUTO'
        DataType = ftInteger
        ParamType = ptInput
        Value = Null
      end>
  end
